{
  "dependencies": {
    "@create-figma-plugin/ui": "^3.2.1",
    "@create-figma-plugin/utilities": "^3.2.1",
    "preact": ">=10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^3.2.1",
    "@create-figma-plugin/tsconfig": "^3.2.1",
    "@figma/plugin-typings": "1.100.2",
    "typescript": ">=4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "api": "1.0.0",
    "editorType": [
      "figma"
    ],
    "id": "figma-organizer",
    "name": "Figma Organizer",
    "menu": [
      {
        "name": "Section Generator",
        "main": "src/section/main.ts",
        "ui": "src/section/ui.tsx"
      },
      "-",
      {
        "name": "Sub Section",
        "main": "src/sub/main.ts",
        "ui": "src/sub/ui.tsx"
      },
      "-",
      {
        "name": "Steps",
        "main": "src/steps/main.ts",
        "ui": "src/steps/ui.tsx"
      }
    ],
    "permissions": [
      "activeusers",
      "currentuser"
    ],
    "relaunchButtons": {
      "section": {
        "name": "Section Generator",
        "main": "src/section/main.ts",
        "ui": "src/section/ui.tsx"
      }
    }
  }
}